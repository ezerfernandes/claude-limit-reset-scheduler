# Google Calendar CLI (calgo) Configuration
# Copy this file to .env and fill in your values

# Path to OAuth2 credentials JSON file downloaded from Google Cloud Console
# Required for authentication
GOOGLE_CALENDAR_CREDENTIALS=./credentials.json

# Path where the OAuth2 token will be stored after first authentication
# This file is created automatically after successful OAuth2 flow
GOOGLE_CALENDAR_TOKEN=./token.json

# Target calendar ID (optional, defaults to "primary")
# Use "primary" for your main calendar, or a specific calendar ID
GOOGLE_CALENDAR_ID=primary
